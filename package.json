{
  "name": "template-vite-react-tauri",
  "private": true,
  "version": "0.0.1",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "tauri": "tauri",
    "eslint": "eslint --ext .ts,.tsx ./src",
    "eslint:fix": "eslint --ext .ts,.tsx ./src --fix",
    "fmt": "prettier --check 'src/**/*.{js,jsx,ts,tsx}'",
    "fmt:fix": "prettier --write 'src/**/*.{js,jsx,ts,tsx}'",
    "lint": "yarn eslint && yarn fmt",
    "lint:fix": "yarn eslint:fix && yarn fmt:fix",
    "tsc": "tsc --noEmit",
    "ci": "yarn lint && yarn tsc",
    "prepare": "husky install"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "bash -c 'yarn lint:fix'"
    ]
  },
  "dependencies": {
    "@tauri-apps/api": "^1.0.1",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "styled-components": "^5.3.5",
    "styled-reset": "^4.4.1"
  },
  "devDependencies": {
    "@tauri-apps/cli": "^1.0.0",
    "@types/node": "^18.0.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@types/styled-components": "^5.1.25",
    "@typescript-eslint/eslint-plugin": "^5.28.0",
    "@typescript-eslint/parser": "^5.28.0",
    "@vitejs/plugin-react": "^1.3.0",
    "eslint": "^7.32.0 || ^8.2.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.2",
    "prettier": "^2.7.1",
    "typescript": "^4.6.3",
    "vite": "^2.9.9"
  }
}
